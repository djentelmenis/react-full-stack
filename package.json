{
  "name": "simple-react-full-stack",
  "version": "0.0.2",
  "description": "Full stack web app starter with React, Node.js, Express and Webpack.",
  "main": "server/index.js",
  "scripts": {
    "build-client": "webpack --config config/webpack.prod.js",
    "build-server": "babel server/index.js -d dist",
    "prod": "npm run build-client && npm run build-server && node dist/index.js",
    "run-client": "webpack-dev-server --hot --config config/webpack.dev.js",
    "run-server": "nodemon --exec babel-node server/index.js",
    "dev": "concurrently \"npm run run-server\" \"npm run run-client\"",
    "test": "nyc mocha --require @babel/register --require ignore-styles --require ./test/bootstrap.js test/**/*.test.js || true",
    "clean": "rm -rf dist/*"
  },
  "author": "djentelmenis",
  "license": "MIT",
  "repository": "https://github.com/djentelmenis/react-full-stack",
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.12.18"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/register": "^7.7.0",
    "@hot-loader/react-dom": "^16.11.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^15.2.1",
    "mocha": "^6.2.2",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.0",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "style-loader": "^1.0.0",
    "url-loader": "^2.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  }
}
