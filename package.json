{
  "name": "react-full-stack",
  "version": "1.0.0",
  "description": "Full stack web app starter with React, Node.js, Express and Webpack.",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run run-server\" \"npm run run-client\"",
    "prod": "npm run build-client && npm run build-server && node dist/index.js",
    "build-client": "webpack --config config/webpack.prod.js",
    "build-server": "babel --extensions .ts,.tsx server/index.ts -d dist",
    "run-client": "webpack-dev-server --hot --config config/webpack.dev.js",
    "run-server": "nodemon",
    "test": "nyc --extension .tsx --extension .ts mocha --require @babel/register --require ignore-styles --require ./test/babel-register.js --require ./test/bootstrap.ts \"./test/**/*.test@(.js|.jsx|.ts|.tsx)\" || true",
    "type-check": "tsc || true",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix || True",
    "clean": "rm -rf dist/*"
  },
  "author": "djentelmenis",
  "license": "MIT",
  "repository": "https://github.com/djentelmenis/react-full-stack",
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "express": "^4.16.3",
    "prop-types": "^15.7.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.12.19"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@hot-loader/react-dom": "^16.11.0",
    "@types/chai": "^4.2.8",
    "@types/chai-enzyme": "^0.6.7",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jsdom": "^12.2.4",
    "@types/mocha": "^7.0.1",
    "@types/sinon": "^7.5.1",
    "@types/sinon-chai": "^3.2.3",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.1",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "install": "^0.13.0",
    "jsdom": "^15.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^6.2.2",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "npm": "^6.13.7",
    "nyc": "^14.1.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.4.0",
    "style-loader": "^1.1.3",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "url-loader": "^2.3.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.2",
    "webpack-merge": "^4.2.2"
  }
}
